<settings>
    <servers>
        <server>
            <id>artifact-registry</id>
            <configuration>
                <httpConfiguration>
                    <get>
                        <usePreemptive>true</usePreemptive>
                    </get>
                    <head>
                        <usePreemptive>true</usePreemptive>
                    </head>
                    <put>
                        <params>
                            <property>
                                <name>http.protocol.expect-continue</name>
                                <value>false</value>
                            </property>
                        </params>
                    </put>
                </httpConfiguration>
            </configuration>
            <username>${GAR_USERNAME}</username>
            <password>${GAR_GEN_OPS_REGISTRY_KEY_B64}</password>
        </server>
    </servers>
</settings>